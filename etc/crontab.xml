<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="forter_forter">
        <job name="forter_send_orders_queue" instance="Forter\Forter\Cron\SendQueue" method="execute">
            <schedule>* * * * *</schedule>
        </job>
    </group>
    <group id="forter_forter_sync">
        <job name="forter_sync_mapper" instance="Forter\Forter\Cron\SyncMapper" method="execute">
            <schedule>0 0 * * */1</schedule>
        </job>
    </group>
</config>
